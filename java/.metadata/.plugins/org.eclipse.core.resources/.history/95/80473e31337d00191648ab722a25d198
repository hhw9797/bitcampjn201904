package test;

import java.io.File;

public class FileTest {

	public static void main(String[] args) {
		
		File f = new File("C:\\myjava\\pen.jpg");
		if (!f.exists()) { // = (f.exists() == false)
			System.out.println("지정하신 폴더에 파일이 존재하지 않습니다.");
			return;
		}
		System.out.println(f.getAbsolutePath());

	}

}
